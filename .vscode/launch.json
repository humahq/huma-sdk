    "version": "0.2.0",
    "configurations": [
        {
            "name": "Webook Callback",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "args": [
                "run",
                "-p",
                "5001",
                "--host",
                "0.0.0.0"
            ],
            "envFile": "${workspaceFolder}/.env",
            "jinja": true,
            "justMyCode": false
        },
        {
            "name": "Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "args": ["exclusion criteria analysis for active phase 3 obesity"]
        }
    ]
}
